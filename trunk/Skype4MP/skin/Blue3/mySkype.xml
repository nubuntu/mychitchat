<window>
   <id>1911</id>
   <defaultcontrol>3</defaultcontrol>
   <allowoverlay>yes</allowoverlay>
   <controls>
	   <import>common.time.xml</import>
	   <control>
		   <description>BG</description>
		   <type>image</type>
		   <id>1</id>
		   <width>720</width>
		   <height>576</height>
	   </control>
	   <control>
		   <type>image</type>
		   <id>1</id>
		   <posX>60</posX>
         <posY>10</posY>
		  <width>80</width>
		  <height>80</height>
		  <zoom>yes</zoom>
		  <texture width="190" height="84">SkypeBlue_128x128.png</texture>
      </control>
      <control>
         <type>group</type>
		  <layout>StackLayout</layout>
		  <description>group element</description>
		  <animation effect="fade" time="250">WindowOpen</animation>
		  <animation effect="fade" time="500">WindowClose</animation>
		  <animation effect="slide" time="250" start="-300,0">WindowOpen</animation>
		  <animation effect="slide" time="500" end="0,-300">WindowClose</animation>
		  <posX>60</posX>
		  <posY>97</posY>
		  <control>
			  <description>status button</description>
			  <type>button</type>
			  <id>3</id>
			  <label>Status</label>
			  <onleft>40</onleft>
			  <onright>41</onright>
			  <onup>13</onup>
			  <ondown>2</ondown>
		  </control>
		  <control>
            <description>SkypeOut Button</description>
            <type>togglebutton</type>
            <id>2</id>
			<label>SkypeOut</label>
            <onup>3</onup>
            <ondown>5</ondown>
            <onleft>40</onleft>
            <onright>41</onright>
         </control>
		  <control>
			  <description>History</description>
			  <type>button</type>
			  <id>5</id>
			  <label>History</label>
			  <onup>2</onup>
			  <ondown>9</ondown>
			  <onleft>40</onleft>
			  <onright>41</onright>
		  </control>
		  <control>
			  <description>Start Skype</description>
			  <type>button</type>
			  <id>9</id>
			  <label>Start Skype</label>
			  <onup>5</onup>
			  <ondown>10</ondown>
			  <onleft>40</onleft>
			  <onright>41</onright>
		  </control>
		  <control>
			  <description>Chats button</description>
			  <type>button</type>
			  <id>10</id>
			  <label>Chats</label>
			  <onup>9</onup>
			  <ondown>11</ondown>
			  <onleft>40</onleft>
			  <onright>41</onright>
		  </control>
		  <control>
			  <description>Silent mode button</description>
			  <type>togglebutton</type>
			  <id>11</id>
			  <label>Silent mode</label>
			  <onup>10</onup>
			  <ondown>13</ondown>
			  <onleft>40</onleft>
			  <onright>41</onright>
		  </control>
		  <control>
			  <description>Settings</description>
			  <type>button</type>
			  <id>13</id>
			  <label>Settings</label>
			  <onup>11</onup>
			  <ondown>14</ondown>
			  <onleft>40</onleft>
			  <onright>41</onright>
		  </control>
		  <!-- For V0.1.1.6 -->
		  <control>
			  <description>Settings</description>
			  <type>button</type>
			  <id>14</id>
			  <label>Audio Devices</label>
			  <onup>13</onup>
			  <ondown>3</ondown>
			  <onleft>40</onleft>
			  <onright>41</onright>
		  </control>
	  </control>
      <control>
         <type>group</type>
         <description>call group</description>
		  <control>
			  <type>image</type>
			  <id>8</id>
			  <posX>260</posX>
			  <posY>95</posY>
			  <width>32</width>
			  <height>32</height>
			  <zoom>false</zoom>
			  <texture>#Skype.Image.MyStatus</texture>
		  </control>
         <control>
            <description>User Status</description>
            <type>label</type>
            <id>12</id>
            <posX>300</posX>
            <posY>97</posY>
            <width>200</width>
            <label>#Skype.User.Status</label>
            <font>font16</font>
            <align>left</align>
            <textcolor>FFFFFFFF</textcolor>
         </control>
